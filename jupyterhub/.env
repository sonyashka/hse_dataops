# JupyterHub Configuration
JUPYTERHUB_PORT=8000
JUPYTERHUB_ADMIN_USER=admin
JUPYTERHUB_ADMIN_PASSWORD=SecureAdminPass123!

# Security Secrets (сгенерируйте реальные значения для продакшена)
JUPYTERHUB_COOKIE_SECRET=super-secret-cookie-key-1234567890-please-change
JUPYTERHUB_PROXY_AUTH_TOKEN=super-secret-proxy-token-1234567890-please-change
JUPYTERHUB_CRYPT_KEY=super-secret-crypt-key-1234567890-please-change

# PostgreSQL Configuration
POSTGRES_USER=jupyterhub_user
POSTGRES_PASSWORD=jupyterhub_password123
POSTGRES_DB=jupyterhub_db
POSTGRES_PORT=5432

# Database URL for JupyterHub (constructed from above)
JUPYTERHUB_DB_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@postgres:5432/${POSTGRES_DB}